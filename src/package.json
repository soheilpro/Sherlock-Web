{
  "name": "sherlock",
  "version": "3.1.1",
  "scripts": {
    "build:ts": "tsc --build tsconfig.json",
    "build": "npm run build:ts",
    "start": "parcel serve ./index.html --dist-dir ../out",
    "publish": "rm -rf ../publish && parcel build ./index.html --dist-dir ../publish --no-cache",
    "clean": "rm -rf ../out",
    "lint:fix": "npm run lint:ts:fix",
    "lint:ts:fix": "eslint --ext .ts,.tsx --fix .",
    "lint:ts": "eslint --ext .ts,.tsx .",
    "lint": "npm run lint:ts"
  },
  "dependencies": {
    "classnames": "2.3.2",
    "crypto-js": "4.2.0",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@parcel/packager-raw-url": "2.7.0",
    "@parcel/transformer-webmanifest": "2.7.0",
    "@types/crypto-js": "4.2.2",
    "@types/gapi": "0.0.35",
    "@types/gapi.auth2": "0.0.47",
    "@types/gapi.client.drive-v3": "0.0.4",
    "@types/google.accounts": "0.0.14",
    "@types/google.picker": "0.0.42",
    "@types/react": "18.0.17",
    "@types/react-dom": "18.0.6",
    "@typescript-eslint/eslint-plugin": "6.7.5",
    "@typescript-eslint/eslint-plugin-tslint": "6.7.5",
    "@typescript-eslint/parser": "6.7.5",
    "autoprefixer": "10.4.0",
    "buffer": "6.0.3",
    "cssnano": "5.0.12",
    "eslint": "8.51.0",
    "eslint-plugin-import": "2.28.1",
    "eslint-plugin-unicorn": "48.0.1",
    "parcel": "2.7.0",
    "postcss": "8.4.31",
    "postcss-cli": "9.1.0",
    "process": "0.11.10",
    "tailwindcss": "3.3.3",
    "tailwindcss-touch": "1.0.1",
    "typescript": "5.2.2"
  }
}
