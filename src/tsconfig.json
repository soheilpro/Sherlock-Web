{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "forceConsistentCasingInFileNames": true,
    "incremental": true,
    "jsx": "react",
    "lib": [ "ES2016", "DOM" ],
    "noEmit": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitOverride": true,
    "noImplicitReturns": true,
    "noPropertyAccessFromIndexSignature": true,
    "noUnusedLocals": true,
    "removeComments": true,
    "rootDir": ".",
    "sourceMap": true,
    "sourceRoot": "/",
    "strict": true,
    "target": "es5",
    "tsBuildInfoFile": "./.ts/.tsbuildinfo"
  },
  "exclude": [
    "node_modules",
  ]
}
